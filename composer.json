{
    "name": "ideanest/student-project-management",
    "description": "A web application for student project management and collaboration",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "IdeaNest Development Team",
            "role": "Developers"
        }
    ],
    "require": {
        "php": "^8.0",
        "phpmailer/phpmailer": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "IdeaNest\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit tests",
        "validate": "composer validate --strict"
    },
    "config": {
        "platform": {
            "php": "8.2.12"
        },
        "preferred-install": {
            "*": "dist"
    }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
